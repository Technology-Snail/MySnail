{
  "name" : "MySnail",
  "short_name" : "üêå",
  "version" : "2.0.0",
  "version_name" : "NOT DONE - DO NOT USE",
  "manifest_version" : 3,
  "minimum_chrome_version" : "4",
  "permissions" : ["storage"],
  "description" : "A customizable snail friend that appears on every website and file you visit with Chrome üêå",
  "homepage_url" : "https://github.com/Technology-Snail/MySnail-Chrome-Extension",
  "author" : {
    "email" : "email.technologysnail@gmail.com",
    "type" : "small business",
    "members" : [
      {
        "name" : "Wesley McGinn",
        "github" : "https://github.com/WesleyMcGinn",
        "email" : "wesleymcginn1@gmail.com"
      }
    ]
  },
  "action" : {
    "default_popup" : "settings.html",
    "default_title" : "MySnail",
    "default_icon" : {
      "16" : "/icon/16.png",
      "32" : "/icon/32.png",
      "48" : "/icon/48.png",
      "128" : "/icon/128.png"
    }
  },
  "icons" : {
    "16" : "/icon/16.png",
    "32" : "/icon/32.png",
    "48" : "/icon/48.png",
    "128" : "/icon/128.png"
  },
  "options_page" : "settings.html",
  "content_scripts" : [
    {
      "js" : ["snail.js"],
      "matches" : [
        "https://*/*",
        "http://*/*",
        "file://*/*"
      ]
    }
  ]
}
